{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n    <li class=\"navUser-item navUser-item-currency\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-downselector-small\" /></svg></i></a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\">\n                    {{#if is_active}}\n                    <strong>{{name}}</strong>\n                    {{else}}\n                    {{name}}\n                    {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n{{/if}}\n","components/common/navigation":"<style>\n.navUser:before, .navUser:after{display:none !important}\n</style>\n<nav class=\"navUser\">\n    <ul class=\"navUser-section navUser-section--alt\">\n        <!--<li class=\"navUser-item navUser-item--account\">\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg><use xlink:href=\"#icon-user\" /></svg>\n            </i>\n            {{#if customer}}\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a> or\n                <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\">{{lang 'common.logout'}}</a>\n            {{else}}\n                <a class=\"navUser-action\" href=\"{{urls.auth.login}}\">{{lang 'common.login'}}</a>\n                {{#if settings.account_creation_enabled}}\n                    <span>{{lang 'common.or'}}</span> <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\">{{lang 'common.sign_up'}}</a>\n                {{/if}}\n            {{/if}}\n        </li>-->\n        {{> components/common/currency-selector}}\n        <li class=\"navUser-item navUser-item--mobile-search\">\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg><use xlink:href=\"#icon-search\" /></svg>\n            </i>\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <a\n                class=\"navUser-action\"\n                data-cart-preview\n                data-dropdown=\"cart-preview-dropdown\"\n                data-options=\"align:right\"\n                href=\"{{urls.cart}}\">\n                <i style=\"margin-right:8px;\" class=\"icon\" aria-hidden=\"true\">\n                  <svg aria-hidden=\"true\" focusable=\"false\" role=\"presentation\" width=\"28\" height=\"26\" viewBox=\"0 10 28 26\" xmlns=\"http://www.w3.org/2000/svg\">\n  <path fill=\"currentColor\" fill-rule=\"evenodd\" d=\"M26.15 14.488L6.977 13.59l-.666-2.661C6.159 10.37 5.704 10 5.127 10H1.213C.547 10 0 10.558 0 11.238c0 .68.547 1.238 1.213 1.238h2.974l3.337 13.249-.82 3.465c-.092.371 0 .774.212 1.053.243.31.576.465.94.465H22.72c.667 0 1.214-.558 1.214-1.239 0-.68-.547-1.238-1.214-1.238H9.434l.333-1.423 12.135-.589c.455-.03.85-.31 1.032-.712l4.247-9.286c.181-.34.151-.774-.06-1.144-.212-.34-.577-.589-.97-.589zM22.297 36c-1.256 0-2.275-1.04-2.275-2.321 0-1.282 1.019-2.322 2.275-2.322s2.275 1.04 2.275 2.322c0 1.281-1.02 2.321-2.275 2.321zM10.92 33.679C10.92 34.96 9.9 36 8.646 36 7.39 36 6.37 34.96 6.37 33.679c0-1.282 1.019-2.322 2.275-2.322s2.275 1.04 2.275 2.322z\"></path>\n</svg>\n                </i>\n                <span class=\"{{#if cart.items}} countPill--positive{{/if}} cart-quantity\">{{cart.quantity}}</span>\n\t\t\t\t<span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        